students = ["Вася", "Маша", "Петя", "Дима", "Марина", "Люба", "Коля", "Ваня"]
grades = {"Вася" : 4, "Петя" : 9, "Марина" : 8, "Люба" : 4, "Коля" : 5, "Ваня": 10}
for i in students:
    if i in grades:
        print(f"{i} оценка:{grades[i]} ")
    else:
        print(f"{i} контрольную работу не писал")
print(" ")  

for i in students:
    if i in grades:
        if grades[i] >= 8:
            print(f"{i}")

good = []
bad = []
for i in students:
    if i in grades:
        if grades[i] >= 6:
            good.append([i])
        else:
            bad.append([i])
print(good)
print(bad)
